{
	"name": "dataflowFechaSql",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_muertes_http_bl",
						"type": "DatasetReference"
					},
					"name": "sourceFecha"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "filterFecha"
				}
			],
			"scriptLines": [
				"source(output(",
				"          country as string,",
				"          country_code as string,",
				"          continent as string,",
				"          population as string,",
				"          indicator as string,",
				"          daily_count as string,",
				"          date as date,",
				"          rate_14_day as string,",
				"          source as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourceFecha",
				"sourceFecha filter(year == '2020') ~> filterFecha"
			]
		}
	}
}