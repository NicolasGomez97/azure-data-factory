{
	"name": "dt_transformacion_muertes",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_muertes_http_bl",
						"type": "DatasetReference"
					},
					"name": "muertesCsvRaw"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "filtermuertes"
				}
			],
			"scriptLines": [
				"source(output(",
				"          country as string,",
				"          country_code as string,",
				"          continent as string,",
				"          population as string,",
				"          indicator as string,",
				"          daily_count as string,",
				"          date as string,",
				"          rate_14_day as string,",
				"          source as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     limit: 0,",
				"     ignoreNoFilesFound: false) ~> muertesCsvRaw",
				"muertesCsvRaw filter(ERROR_FUNCTION('')) ~> filtermuertes"
			]
		}
	}
}