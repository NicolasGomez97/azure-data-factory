{
	"name": "dataFlowTransformacionMuertes",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_muertes_http_bl",
						"type": "DatasetReference"
					},
					"name": "muertesCsvRaw"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "filtermuertes"
				},
				{
					"name": "selectioneuropemuerte"
				},
				{
					"name": "muertespivot"
				}
			],
			"scriptLines": [
				"source(output(",
				"          country as string,",
				"          country_code as string,",
				"          continent as string,",
				"          population as string,",
				"          indicator as string,",
				"          daily_count as string,",
				"          date as string,",
				"          rate_14_day as string,",
				"          source as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     limit: 0,",
				"     ignoreNoFilesFound: false) ~> muertesCsvRaw",
				"muertesCsvRaw filter(continent == 'Europe') ~> filtermuertes",
				"filtermuertes select(mapColumn(",
				"          country,",
				"          country_code,",
				"          population,",
				"          indicator,",
				"          daily_count,",
				"          date_process = date,",
				"          source,",
				"          each(match(true()),",
				"               'casos_'+$$ = $$)",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> selectioneuropemuerte",
				"selectioneuropemuerte pivot(groupBy(indicator,",
				"          country_code,",
				"          population,",
				"          daily_count,",
				"          date_process,",
				"          source),",
				"     columnNaming: '$N$V',",
				"     lateral: true) ~> muertespivot"
			]
		}
	}
}